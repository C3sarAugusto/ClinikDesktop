join wof_medicos m on w.solicitante_id=m.medico_id
where  data_lancamento between $P{dataDe} and $P{dataAte} and (( w.solicitante_id=$P{medicoId}) or (0=$P{medicoId})) and deletado=false
group by medico, exame]]>
